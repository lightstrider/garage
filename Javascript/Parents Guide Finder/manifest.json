{
  "manifest_version": 2,
  "name": "Parents Guide Finder",
  "version": "0.1",
  "description":"Enter movie name to find parents guide",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Parents Guide Finder",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}
